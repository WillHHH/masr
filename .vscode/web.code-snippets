			"import { useRouter } from \"next/router\";",
			"",
			"export interface ${1:name}Props {",
			"\t${2:pageProp}: ${3:fetchedDataType};",
			"}",
			"",
			"const ${1:name}:FC<${1:name}Props> = ({ ${2:pageProp} }) => {",
