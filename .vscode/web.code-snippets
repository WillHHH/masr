			"\t\t<Page meta={meta}>",
			"\t\t\t",
			"\t\t</Page>",
			"\t);",
			"};",
			"",
			"export default ${1:name};",
			"",
			"export async function getStaticProps({params}) {",
			"\tconst ${2:pageProp}s = await fetchCollectionType(\"${2:pageProp}s\", {",
			"\t\tslug: params.slug,",
			"\t});",
			"",
			"\treturn ${2:pageProp}s.length",
			"\t\t? {",
			"\t\t\t\tprops: {",
